<h1>Users</h1>
<% @users.each do |user| %>
    <%= user.full_name %><br/>
<% end %>

<h1>Articles</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Author</th>
    <th>Action</th>
  </tr>
  <% @articles.each do |article| %>
      <tr>
        <td><%= link_to article.title, article_path(article) %></td>
        <td><%= article.content %></td>
        <td><%= article.user.full_name %></td>
        <td><%= link_to "Edit", edit_article_path(article) %> | <%= link_to "Delete", article_path(article), method: :delete %></td>
      </tr>
  <% end %>
</table>
<%= link_to "New", new_article_path %> | <%= link_to "All", articles_path %>