<% if has_rights?('user', 'index') %>
    <h1>Users</h1>
    <% @users.each do |user| %>
        <%= user.full_name %><br/>
    <% end %>
<% end %>


<% if has_rights?('article', 'index') %>
    <h1>Articles</h1>
    <%= render partial: "articles/articles" %>
    <%= link_to "All", articles_path %>
<% end %>